{
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "http://54.75.138.9:8100/api/v1/:path*"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
